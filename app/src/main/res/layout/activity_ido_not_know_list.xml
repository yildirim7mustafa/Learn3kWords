<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profilebcg"
    tools:context=".IDoNotKnowListActivity">

    <TextView
        android:id="@+id/iDoNotKnowListTextView"
        android:layout_width="312dp"
        android:layout_height="72dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/bt_bg1"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16" />

    <Button
        android:id="@+id/iDoNotKnowListPassButton"
        android:layout_width="263dp"
        android:layout_height="74dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="74dp"
        android:text="PASS"
        android:background="@drawable/bt_bg"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iDoNotKnowListTextView"
        app:layout_constraintVertical_bias="0.012" />

    <Button
        android:id="@+id/iDoNotKnowListLearnedButton"
        android:layout_width="172dp"
        android:layout_height="62dp"
        android:layout_marginStart="119dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="120dp"
        android:text="OGRENDIM"
        android:background="@drawable/bt_bg"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iDoNotKnowListPassButton"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/iDoNotKnowListBackButton"
        android:layout_width="106dp"
        android:layout_height="59dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="153dp"
        android:text="BACK"
        android:background="@drawable/bt_bg"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iDoNotKnowListLearnedButton"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>